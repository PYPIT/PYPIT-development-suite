# This is a comment line

# Change the default settings
run ncpus 4
output verbosity 2
output overwrite True
run spectrograph keck_deimos
run calcheck False
output sorted keck_deimos_setup_A

# Reduce
reduce calibrate flux False # Do not flux calibrate 

# Slit tracing
#trace slits sigdetect 50.0
#trace slits number -1
#trace slits pca params [3,2,1,0]
#trace slits polyorder  3
#trace slits tilts params 1,1,1
#trace slits tilts method spca
#trace slits sobel mode nearest
trace slits fracignore 0.02  # Ignore star boxes
bias useframe overscan
pixelflat combine method median
pixelflat combine reject level [10.0,10.0]

reduce detnum 1

# Spect
spect read
 arc number 1
spect end

# Setup
setup read
 Setup A
 --:
   dichroic: None
   disperser: {angle: 8500.0078125, name: 830G}
   slit: {decker: CFHQS1B, slitlen: None, slitwid: None}
 '01': {binning: '1,1', det: 1, namp: 1}
 '02': {binning: '1,1', det: 2, namp: 1}
 '03': {binning: '1,1', det: 3, namp: 1}
 '04': {binning: '1,1', det: 4, namp: 1}
 '05': {binning: '1,1', det: 5, namp: 1}
 '06': {binning: '1,1', det: 6, namp: 1}
 '07': {binning: '1,1', det: 7, namp: 1}
 '08': {binning: '1,1', det: 8, namp: 1}
setup end

